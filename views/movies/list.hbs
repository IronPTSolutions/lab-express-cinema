<div class="wrap">
    <div class="row">
    <div class="card" style="width: 18rem;">
        {{#each movies as |movie|}} 
        <img src="{{movie.image}}" class="card-img-top" alt="{{movie.title}}" />
        <div class="card-body">
        <h5 class="card-title">{{movie.title}}</h5>
        <a href="/movies/{{movie.id}}" class="btn btn-primary">See more </a>
        </div>
    </div>
    {{/each}}
    </div>
</div>

